
[1000]

&quan &i in toSet(&x), &guard . &body

    ~~>

&quan j0 : &index0 .
    &quan j1 : &index1
        , &guard { &i --> (j0, j1) } /\ &refnx[j0,j1]
        . &body  { &i --> (j0, j1) }

    where &x hasRepr RelationIntMatrix2

    letting &refnx be refn(&x)
    letting &index0 be indices(&refnx, 0)
    letting &index1 be indices(&refnx, 1)

