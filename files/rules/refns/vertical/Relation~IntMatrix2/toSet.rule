
[1000]

&quan &i in toSet(&rel), &guard . &body

~~>

&quan j1 : &index1 .
    &quan j2 : &index2
        , &guard { &i --> (j1, j2) } /\ &refnrel[j1,j2]
        . &body  { &i --> (j1, j2) }

    where &rel hasRepr RelationIntMatrix2




    letting &refnrel be refn(&rel)
    letting &index1  be indices(&refnrel, 0)
    letting &index2  be indices(&refnrel, 1)

