find v__5: int(0..10 + n)
such that
    exists v__3 : int(0..10 + n) . x_Occurrence[v__3] /\ v__3 = 2,
    8 < v__5,
    (sum v__1 : int(0..10 + n) . x_Occurrence[v__1]) <= 2,
    forAll v__6 : int(0..10 + n) . x_Occurrence[v__6] -> v__6 <= v__5,
    exists v__8 : int(0..10 + n) . x_Occurrence[v__8] /\ v__8 = v__5
