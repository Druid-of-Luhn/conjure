    and([q2 <= x_ExplicitVarSizeWithMarker_Marker -> x_Occurrence[x_ExplicitVarSizeWithMarker_Values[q2]] | q2 : int(1..4)]),
    and([x_Occurrence[q8] -> or([x_ExplicitVarSizeWithMarker_Values[q6] = q8 /\ q6 <= x_ExplicitVarSizeWithMarker_Marker | q6 : int(1..4)]) | q8 : int(1..4)]),
    and([x_Occurrence[q10] -> or([x_ExplicitVarSizeWithMarker_Values[q7] = q10 /\ q7 <= x_ExplicitVarSizeWithMarker_Marker | q7 : int(1..4)]) | q10 : int(1..4)]),
    and([q9 <= x_ExplicitVarSizeWithMarker_Marker -> x_Occurrence[x_ExplicitVarSizeWithMarker_Values[q9]] | q9 : int(1..4)])

