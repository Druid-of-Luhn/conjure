#/bin/sh

find . -name "*.prof" -delete
find . -name "*.hi" -delete
find . -name "*.o" -delete
find . -name "*.hi-boot" -delete
find . -name "*.o-boot" -delete
rm -f src/RepositoryVersion.hs
cabal clean                   && echo "cabal clean successful"  || echo "cabal clean failed"
ghc-pkg unregister conjure-cp && echo "conjure-cp unregistered" || echo "no need to unregister conjure-cp"

