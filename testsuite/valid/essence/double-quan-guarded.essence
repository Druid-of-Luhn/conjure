language Essence 2.0

letting myForall be quantifier {
                                append   { x:bool, y:bool --> x /\ y }
                                guard    { x:bool, y:bool --> x -> y }
                                identity true
                             }

find s : set of int(1..90)

such that
    myForall i,j : int(1..9), i = 2*j . i+j in s
