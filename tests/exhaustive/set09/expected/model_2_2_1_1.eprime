             | q11 : int(2..5)]),
    and([q15 <= x_ExplicitVarSizeWithMarker_Marker ->
         x_Occurrence[x_ExplicitVarSizeWithMarker_Values[q15]]
             | q15 : int(1..4)]),
    1 <= sum([y_Occurrence[q8] | q8 : int(2..5)]),
    and([y_Occurrence[q16] ->
         or([q18 <= y_ExplicitVarSizeWithMarker_Marker /\
             y_ExplicitVarSizeWithMarker_Values[q18] = q16
                 | q18 : int(1..4)])
             | q16 : int(2..5)]),
    and([q20 <= y_ExplicitVarSizeWithMarker_Marker ->
         y_Occurrence[y_ExplicitVarSizeWithMarker_Values[q20]]
             | q20 : int(1..4)])

