                 | q14 : int(1..4)])
             | q12 : int(1..4)]),
    and([x_ExplicitVarSizeWithFlags_Flags[q16] ->
         or([q18 <= x_ExplicitVarSizeWithMarker_Marker /\
             x_ExplicitVarSizeWithMarker_Values[q18] = x_ExplicitVarSizeWithFlags_Values[q16]
                 | q18 : int(1..4)])
             | q16 : int(1..4)])

