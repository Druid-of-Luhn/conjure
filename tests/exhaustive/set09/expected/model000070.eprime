    and([x_ExplicitVarSizeWithFlags_Flags[q1] = false -> x_ExplicitVarSizeWithFlags_Values[q1] = 2 | q1 : int(1..1 + (5 - 2))]),
    and([x_ExplicitVarSizeWithFlags_Flags[q1 + 1] -> x_ExplicitVarSizeWithFlags_Flags[q1] | q1 : int(1..1 + (5 - 2) - 1)]),
    1 <= sum([x_ExplicitVarSizeWithFlags_Flags[q1] | q1 : int(1..1 + (5 - 2))]),
