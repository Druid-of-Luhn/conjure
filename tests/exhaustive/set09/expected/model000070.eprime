such that
    and([x_ExplicitVarSizeWithFlags_Flags[i] /\ j <= y_ExplicitVarSizeWithMarker_Marker ->
         x_ExplicitVarSizeWithFlags_Values[i] + 2 = y_ExplicitVarSizeWithMarker_Values[j]
