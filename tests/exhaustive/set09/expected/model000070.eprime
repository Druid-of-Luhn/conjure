    and([y_Occurrence[q4] -> or([q5 <= y_ExplicitVarSizeWithMarker_Marker /\ y_ExplicitVarSizeWithMarker_Values[q5] = q4 | q5 : int(1..1 + (5 - 2))])
             | q4 : int(2..5)]),
