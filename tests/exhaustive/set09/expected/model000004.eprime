language ESSENCE' 1.0

find x_Occurrence: matrix indexed by [int(2..5)] of bool
find x_ExplicitVarSizeWithMarker_Marker: int(0..1 + (5 - 2))
find x_ExplicitVarSizeWithMarker_Values: matrix indexed by [int(1..1 + (5 - 2))] of int(2..5)
find y_Occurrence: matrix indexed by [int(2..5)] of bool
such that
    and([x_Occurrence[i] /\ y_Occurrence[j] -> i + 2 = j | i : int(2..5), j : int(2..5)]),
    1 <= sum([x_Occurrence[q1] | q1 : int(2..5)]),
    1 <= sum([y_Occurrence[q2] | q2 : int(2..5)]),
    and([q3 + 1 <= x_ExplicitVarSizeWithMarker_Marker -> x_ExplicitVarSizeWithMarker_Values[q3] < x_ExplicitVarSizeWithMarker_Values[q3 + 1]
             | q3 : int(1..1 + (5 - 2) - 1)]),
    and([q3 > x_ExplicitVarSizeWithMarker_Marker -> x_ExplicitVarSizeWithMarker_Values[q3] = 2 | q3 : int(1..1 + (5 - 2))]),
    1 <= x_ExplicitVarSizeWithMarker_Marker,
    and([q5 <= x_ExplicitVarSizeWithMarker_Marker -> x_Occurrence[x_ExplicitVarSizeWithMarker_Values[q5]] | q5 : int(1..1 + (5 - 2))]),
    and([x_Occurrence[q4] -> or([q7 <= x_ExplicitVarSizeWithMarker_Marker /\ x_ExplicitVarSizeWithMarker_Values[q7] = q4 | q7 : int(1..1 + (5 - 2))])
             | q4 : int(2..5)])

