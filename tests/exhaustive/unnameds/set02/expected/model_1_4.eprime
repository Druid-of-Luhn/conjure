language ESSENCE' 1.0

given n: int
find s_Occurrence: matrix indexed by [int(1..n)] of bool
find s_ExplicitVarSizeWithFlags_Flags: matrix indexed by [int(1..n)] of bool
find s_ExplicitVarSizeWithFlags_Values: matrix indexed by [int(1..n)] of int(1..n)
such that
    and([[s_Occurrence[[q2, q1 + 1, q1; int(0..2)][toInt(q2 = q1) + 2 * toInt(q2 = q1 + 1)]] | q2 : int(1..n)] <=lex
         [s_Occurrence[q2] | q2 : int(1..n)]
             | q1 : int(1..n - 1)]),
    and([s_ExplicitVarSizeWithFlags_Flags[q4 + 1] ->
         s_ExplicitVarSizeWithFlags_Values[q4] < s_ExplicitVarSizeWithFlags_Values[q4 + 1]
             | q4 : int(1..n - 1)]),
    and([s_ExplicitVarSizeWithFlags_Flags[q5] = false -> s_ExplicitVarSizeWithFlags_Values[q5] = 1 | q5 : int(1..n)]),
    and([s_ExplicitVarSizeWithFlags_Flags[q6 + 1] -> s_ExplicitVarSizeWithFlags_Flags[q6] | q6 : int(1..n - 1)]),
    and([s_ExplicitVarSizeWithFlags_Flags[q10] -> s_Occurrence[s_ExplicitVarSizeWithFlags_Values[q10]]
             | q10 : int(1..n)]),
    and([s_Occurrence[q11] ->
         or([s_ExplicitVarSizeWithFlags_Flags[q13] /\ s_ExplicitVarSizeWithFlags_Values[q13] = q11 | q13 : int(1..n)])
             | q11 : int(1..n)])

