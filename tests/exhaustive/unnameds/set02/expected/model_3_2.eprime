language ESSENCE' 1.0

given n: int
find s_ExplicitVarSizeWithMarker_Marker: int(0..1 + (n - 1))
find s_ExplicitVarSizeWithMarker_Values: matrix indexed by [int(1..1 + (n - 1))] of int(1..n)
find s_ExplicitVarSizeWithDummy: matrix indexed by [int(1..1 + (n - 1))] of int(1..n + 1)
such that
    and([[or([q17 <= s_ExplicitVarSizeWithMarker_Marker /\ s_ExplicitVarSizeWithMarker_Values[q17] = q3
                  | q17 : int(1..1 + (n - 1))])
              | q2 : int(1..n), q3 : int(1..n), q2 = q1 -> q3 = q1 + 1, q2 = q1 + 1 -> q3 = q1,
                q2 != q1 /\ q2 != q1 + 1 -> q3 = q2]
         <=lex
         [or([q19 <= s_ExplicitVarSizeWithMarker_Marker /\ s_ExplicitVarSizeWithMarker_Values[q19] = q2
                  | q19 : int(1..1 + (n - 1))])
              | q2 : int(1..n)]
             | q1 : int(1..n - 1)]),
    and([q1 + 1 <= s_ExplicitVarSizeWithMarker_Marker ->
         s_ExplicitVarSizeWithMarker_Values[q1] < s_ExplicitVarSizeWithMarker_Values[q1 + 1]
             | q1 : int(1..1 + (n - 1) - 1)]),
    and([q2 > s_ExplicitVarSizeWithMarker_Marker -> s_ExplicitVarSizeWithMarker_Values[q2] = 1
             | q2 : int(1..1 + (n - 1))]),
    and([s_ExplicitVarSizeWithDummy[q4] < s_ExplicitVarSizeWithDummy[q4 + 1] \/ s_ExplicitVarSizeWithDummy[q4] = n + 1
             | q4 : int(1..1 + (n - 1) - 1)]),
    and([s_ExplicitVarSizeWithDummy[q5] = n + 1 -> s_ExplicitVarSizeWithDummy[q5 + 1] = n + 1
             | q5 : int(1..1 + (n - 1) - 1)]),
    and([s_ExplicitVarSizeWithDummy[q9] != n + 1 ->
         or([q11 <= s_ExplicitVarSizeWithMarker_Marker /\
             s_ExplicitVarSizeWithMarker_Values[q11] = s_ExplicitVarSizeWithDummy[q9]
                 | q11 : int(1..1 + (n - 1))])
             | q9 : int(1..1 + (n - 1))]),
    and([q13 <= s_ExplicitVarSizeWithMarker_Marker ->
         or([s_ExplicitVarSizeWithDummy[q15] != n + 1 /\
             s_ExplicitVarSizeWithDummy[q15] = s_ExplicitVarSizeWithMarker_Values[q13]
                 | q15 : int(1..1 + (n - 1))])
             | q13 : int(1..1 + (n - 1))])

