language ESSENCE' 1.0

given n: int
find s_ExplicitVarSizeWithDummy: matrix indexed by [int(1..n)] of int(1..n + 1)
find s_Occurrence: matrix indexed by [int(1..n)] of bool
such that
    and([[or([s_ExplicitVarSizeWithDummy[q15] != n + 1 /\
              s_ExplicitVarSizeWithDummy[q15] = [q2, q1 + 1, q1; int(0..2)][toInt(q2 = q1) + 2 * toInt(q2 = q1 + 1)]
                  | q15 : int(1..n)])
              | q2 : int(1..n)]
         <=lex
         [or([s_ExplicitVarSizeWithDummy[q18] != n + 1 /\ s_ExplicitVarSizeWithDummy[q18] = q2 | q18 : int(1..n)])
              | q2 : int(1..n)]
             | q1 : int(1..n - 1)]),
    and([s_ExplicitVarSizeWithDummy[q3] < s_ExplicitVarSizeWithDummy[q3 + 1] \/ s_ExplicitVarSizeWithDummy[q3] = n + 1
             | q3 : int(1..n - 1)]),
    and([s_ExplicitVarSizeWithDummy[q4] = n + 1 -> s_ExplicitVarSizeWithDummy[q4 + 1] = n + 1 | q4 : int(1..n - 1)]),
    and([s_Occurrence[q8] ->
         or([s_ExplicitVarSizeWithDummy[q10] != n + 1 /\ s_ExplicitVarSizeWithDummy[q10] = q8 | q10 : int(1..n)])
             | q8 : int(1..n)]),
    and([s_ExplicitVarSizeWithDummy[q12] != n + 1 -> s_Occurrence[s_ExplicitVarSizeWithDummy[q12]] | q12 : int(1..n)])

