language ESSENCE' 1.0

given n: int
find s_ExplicitVarSizeWithDummy: matrix indexed by [int(1..n)] of int(1..n + 1)
find s_ExplicitVarSizeWithMarker_Marker: int(0..n)
find s_ExplicitVarSizeWithMarker_Values: matrix indexed by [int(1..n)] of int(1..n)
such that
    and([[or([s_ExplicitVarSizeWithDummy[q20] != n + 1 /\
              s_ExplicitVarSizeWithDummy[q20] = [q2, q1 + 1, q1; int(0..2)][toInt(q2 = q1) + 2 * toInt(q2 = q1 + 1)]
                  | q20 : int(1..n)])
              | q2 : int(1..n)]
         <=lex
         [or([s_ExplicitVarSizeWithDummy[q23] != n + 1 /\ s_ExplicitVarSizeWithDummy[q23] = q2 | q23 : int(1..n)])
              | q2 : int(1..n)]
             | q1 : int(1..n - 1)]),
    and([s_ExplicitVarSizeWithDummy[q3] < s_ExplicitVarSizeWithDummy[q3 + 1] \/ s_ExplicitVarSizeWithDummy[q3] = n + 1
             | q3 : int(1..n - 1)]),
    and([s_ExplicitVarSizeWithDummy[q4] = n + 1 -> s_ExplicitVarSizeWithDummy[q4 + 1] = n + 1 | q4 : int(1..n - 1)]),
    and([q7 + 1 <= s_ExplicitVarSizeWithMarker_Marker ->
         s_ExplicitVarSizeWithMarker_Values[q7] < s_ExplicitVarSizeWithMarker_Values[q7 + 1]
             | q7 : int(1..n - 1)]),
    and([q8 > s_ExplicitVarSizeWithMarker_Marker -> s_ExplicitVarSizeWithMarker_Values[q8] = 1 | q8 : int(1..n)]),
    and([q11 <= s_ExplicitVarSizeWithMarker_Marker ->
         or([s_ExplicitVarSizeWithDummy[q13] != n + 1 /\
             s_ExplicitVarSizeWithDummy[q13] = s_ExplicitVarSizeWithMarker_Values[q11]
                 | q13 : int(1..n)])
             | q11 : int(1..n)]),
    and([s_ExplicitVarSizeWithDummy[q15] != n + 1 ->
         or([q17 <= s_ExplicitVarSizeWithMarker_Marker /\
             s_ExplicitVarSizeWithMarker_Values[q17] = s_ExplicitVarSizeWithDummy[q15]
                 | q17 : int(1..n)])
             | q15 : int(1..n)])

