language ESSENCE' 1.0

given n: int
find s_ExplicitVarSizeWithDummy:
        matrix indexed by [int(1..1 + (n - 1))] of int(1..n + 1)
find s_ExplicitVarSizeWithMarker_Marker: int(0..1 + (n - 1))
find s_ExplicitVarSizeWithMarker_Values:
        matrix indexed by [int(1..1 + (n - 1))] of int(1..n)
such that
    and([[or([s_ExplicitVarSizeWithDummy[q17] != n + 1 /\
              s_ExplicitVarSizeWithDummy[q17] = q3
                  | q17 : int(1..1 + (n - 1))])
              | q2 : int(1..n), q3 : int(1..n), q2 = q1 -> q3 = q1 + 1,
                q2 = q1 + 1 -> q3 = q1, q2 != q1 /\ q2 != q1 + 1 -> q3 = q2]
         <=lex
         [or([s_ExplicitVarSizeWithDummy[q19] != n + 1 /\
              s_ExplicitVarSizeWithDummy[q19] = q2
                  | q19 : int(1..1 + (n - 1))])
              | q2 : int(1..n)]
             | q1 : int(1..n - 1)]),
    and([s_ExplicitVarSizeWithDummy[q1] < s_ExplicitVarSizeWithDummy[q1 + 1] \/
         s_ExplicitVarSizeWithDummy[q1] = n + 1
             | q1 : int(1..1 + (n - 1) - 1)]),
    and([s_ExplicitVarSizeWithDummy[q2] = n + 1 ->
         s_ExplicitVarSizeWithDummy[q2 + 1] = n + 1
             | q2 : int(1..1 + (n - 1) - 1)]),
    and([q5 + 1 <= s_ExplicitVarSizeWithMarker_Marker ->
         s_ExplicitVarSizeWithMarker_Values[q5] <
         s_ExplicitVarSizeWithMarker_Values[q5 + 1]
             | q5 : int(1..1 + (n - 1) - 1)]),
    and([q6 > s_ExplicitVarSizeWithMarker_Marker ->
         s_ExplicitVarSizeWithMarker_Values[q6] = 1
             | q6 : int(1..1 + (n - 1))]),
    and([q9 <= s_ExplicitVarSizeWithMarker_Marker ->
         or([s_ExplicitVarSizeWithDummy[q11] != n + 1 /\
             s_ExplicitVarSizeWithDummy[q11] = s_ExplicitVarSizeWithMarker_Values[q9]
                 | q11 : int(1..1 + (n - 1))])
             | q9 : int(1..1 + (n - 1))]),
    and([s_ExplicitVarSizeWithDummy[q13] != n + 1 ->
         or([q15 <= s_ExplicitVarSizeWithMarker_Marker /\
             s_ExplicitVarSizeWithMarker_Values[q15] = s_ExplicitVarSizeWithDummy[q13]
                 | q15 : int(1..1 + (n - 1))])
             | q13 : int(1..1 + (n - 1))])

