language ESSENCE' 1.0

given n: int
find s_ExplicitVarSizeWithMarker_Marker: int(0..n)
find s_ExplicitVarSizeWithMarker_Values: matrix indexed by [int(1..n)] of int(1..n)
find s_ExplicitVarSizeWithFlags_Flags: matrix indexed by [int(1..n)] of bool
find s_ExplicitVarSizeWithFlags_Values: matrix indexed by [int(1..n)] of int(1..n)
such that
    and([[or([q21 <= s_ExplicitVarSizeWithMarker_Marker /\
              s_ExplicitVarSizeWithMarker_Values[q21] =
              [q2, q1 + 1, q1; int(0..2)][toInt(q2 = q1) + 2 * toInt(q2 = q1 + 1)]
                  | q21 : int(1..n)])
              | q2 : int(1..n)]
         <=lex
         [or([q24 <= s_ExplicitVarSizeWithMarker_Marker /\ s_ExplicitVarSizeWithMarker_Values[q24] = q2
                  | q24 : int(1..n)])
              | q2 : int(1..n)]
             | q1 : int(1..n - 1)]),
    and([q3 + 1 <= s_ExplicitVarSizeWithMarker_Marker ->
         s_ExplicitVarSizeWithMarker_Values[q3] < s_ExplicitVarSizeWithMarker_Values[q3 + 1]
             | q3 : int(1..n - 1)]),
    and([q4 > s_ExplicitVarSizeWithMarker_Marker -> s_ExplicitVarSizeWithMarker_Values[q4] = 1 | q4 : int(1..n)]),
    and([s_ExplicitVarSizeWithFlags_Flags[q6 + 1] ->
         s_ExplicitVarSizeWithFlags_Values[q6] < s_ExplicitVarSizeWithFlags_Values[q6 + 1]
             | q6 : int(1..n - 1)]),
    and([s_ExplicitVarSizeWithFlags_Flags[q7] = false -> s_ExplicitVarSizeWithFlags_Values[q7] = 1 | q7 : int(1..n)]),
    and([s_ExplicitVarSizeWithFlags_Flags[q8 + 1] -> s_ExplicitVarSizeWithFlags_Flags[q8] | q8 : int(1..n - 1)]),
    and([s_ExplicitVarSizeWithFlags_Flags[q12] ->
         or([q14 <= s_ExplicitVarSizeWithMarker_Marker /\
             s_ExplicitVarSizeWithMarker_Values[q14] = s_ExplicitVarSizeWithFlags_Values[q12]
                 | q14 : int(1..n)])
             | q12 : int(1..n)]),
    and([q16 <= s_ExplicitVarSizeWithMarker_Marker ->
         or([s_ExplicitVarSizeWithFlags_Flags[q18] /\
             s_ExplicitVarSizeWithFlags_Values[q18] = s_ExplicitVarSizeWithMarker_Values[q16]
                 | q18 : int(1..n)])
             | q16 : int(1..n)])

