language ESSENCE' 1.0

letting n be 5
find s_ExplicitWithRepetition_Flag: int(0..6)
find s_ExplicitWithRepetition_Values: matrix indexed by [int(1..6)] of int(1..5)
such that
    and([[sum([toInt(q8 <= s_ExplicitWithRepetition_Flag) *
               toInt(s_ExplicitWithRepetition_Values[q8] =
                     [q2, q1 + 1, q1; int(0..2)][toInt(q2 = q1) + 2 * toInt(q2 = q1 + 1)])
                   | q8 : int(1..6)])
              | q2 : int(1..5)]
         <=lex
         [sum([toInt(q11 <= s_ExplicitWithRepetition_Flag) * toInt(s_ExplicitWithRepetition_Values[q11] = q2)
                   | q11 : int(1..6)])
              | q2 : int(1..5)]
             | q1 : int(1..4)]),
    and([q1 + 1 <= s_ExplicitWithRepetition_Flag ->
         s_ExplicitWithRepetition_Values[q1] <= s_ExplicitWithRepetition_Values[q1 + 1]
             | q1 : int(1..5)]),
    and([q2 > s_ExplicitWithRepetition_Flag -> s_ExplicitWithRepetition_Values[q2] = 1 | q2 : int(1..6)]),
    and([q4 <= s_ExplicitWithRepetition_Flag ->
         sum([toInt(q15 <= s_ExplicitWithRepetition_Flag) *
              toInt(s_ExplicitWithRepetition_Values[q15] = s_ExplicitWithRepetition_Values[q4])
                  | q15 : int(1..6)])
         <= 2
             | q4 : int(1..6)]),
    s_ExplicitWithRepetition_Flag <= 3

