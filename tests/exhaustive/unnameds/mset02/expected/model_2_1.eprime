language ESSENCE' 1.0

given n: int
find s_ExplicitWithRepetition_Flag: int(0..2 * n * 2)
find s_ExplicitWithRepetition_Values: matrix indexed by [int(1..2 * n * 2)] of int(1..n)
find s_ExplicitWithFlags_Flags: matrix indexed by [int(1..2 * n)] of int(0..2)
find s_ExplicitWithFlags_Values: matrix indexed by [int(1..2 * n)] of int(1..n)
such that
    and([[sum([toInt(q27 <= s_ExplicitWithRepetition_Flag) *
               toInt(s_ExplicitWithRepetition_Values[q27] =
                     [q2, q1 + 1, q1; int(0..2)][toInt(q2 = q1) + 2 * toInt(q2 = q1 + 1)])
                   | q27 : int(1..2 * n * 2)])
              | q2 : int(1..n)]
         <=lex
         [sum([toInt(q30 <= s_ExplicitWithRepetition_Flag) * toInt(s_ExplicitWithRepetition_Values[q30] = q2)
                   | q30 : int(1..2 * n * 2)])
              | q2 : int(1..n)]
             | q1 : int(1..n - 1)]),
    and([q3 + 1 <= s_ExplicitWithRepetition_Flag ->
         s_ExplicitWithRepetition_Values[q3] <= s_ExplicitWithRepetition_Values[q3 + 1]
             | q3 : int(1..2 * n * 2 - 1)]),
    and([q4 > s_ExplicitWithRepetition_Flag -> s_ExplicitWithRepetition_Values[q4] = 1 | q4 : int(1..2 * n * 2)]),
    and([q6 <= s_ExplicitWithRepetition_Flag ->
         sum([toInt(q15 <= s_ExplicitWithRepetition_Flag) *
              toInt(s_ExplicitWithRepetition_Values[q15] = s_ExplicitWithRepetition_Values[q6])
                  | q15 : int(1..2 * n * 2)])
         <= 2
             | q6 : int(1..2 * n * 2)]),
    and([s_ExplicitWithFlags_Flags[q8 + 1] > 0 -> s_ExplicitWithFlags_Values[q8] < s_ExplicitWithFlags_Values[q8 + 1]
             | q8 : int(1..2 * n - 1)]),
    and([s_ExplicitWithFlags_Flags[q9] = 0 -> s_ExplicitWithFlags_Values[q9] = 1 | q9 : int(1..2 * n)]),
    and([s_ExplicitWithFlags_Flags[q10 + 1] > 0 -> s_ExplicitWithFlags_Flags[q10] > 0 | q10 : int(1..2 * n - 1)]),
    and([s_ExplicitWithFlags_Flags[q17] > 0 ->
         sum([toInt(s_ExplicitWithFlags_Values[q18] = s_ExplicitWithFlags_Values[q17]) * s_ExplicitWithFlags_Flags[q18]
                  | q18 : int(1..2 * n)])
         =
         sum([toInt(q20 <= s_ExplicitWithRepetition_Flag) *
              toInt(s_ExplicitWithRepetition_Values[q20] = s_ExplicitWithFlags_Values[q17])
                  | q20 : int(1..2 * n * 2)])
             | q17 : int(1..2 * n)]),
    and([q21 <= s_ExplicitWithRepetition_Flag ->
         sum([toInt(s_ExplicitWithFlags_Values[q22] = s_ExplicitWithRepetition_Values[q21]) *
              s_ExplicitWithFlags_Flags[q22]
                  | q22 : int(1..2 * n)])
         =
         sum([toInt(q24 <= s_ExplicitWithRepetition_Flag) *
              toInt(s_ExplicitWithRepetition_Values[q24] = s_ExplicitWithRepetition_Values[q21])
                  | q24 : int(1..2 * n * 2)])
             | q21 : int(1..2 * n * 2)])

