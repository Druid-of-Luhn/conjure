    and([s_ExplicitWithFlags_Flags[q16 + 1] > 0 -> s_ExplicitWithFlags_Values[q16] < s_ExplicitWithFlags_Values[q16 + 1]
             | q16 : int(1..5)]),
    and([s_ExplicitWithFlags_Flags[q17] = 0 -> s_ExplicitWithFlags_Values[q17] = 1 | q17 : int(1..6)]),
    and([s_ExplicitWithFlags_Flags[q18 + 1] > 0 -> s_ExplicitWithFlags_Flags[q18] > 0 | q18 : int(1..5)]),
    and([s_ExplicitWithFlags_Flags[q23] > 0 ->
         sum([toInt(s_ExplicitWithFlags_Values[q24] = s_ExplicitWithFlags_Values[q23]) * s_ExplicitWithFlags_Flags[q24]
                  | q24 : int(1..6)])
         =
         sum([toInt(q26 <= s_ExplicitWithRepetition_Flag) *
              toInt(s_ExplicitWithRepetition_Values[q26] = s_ExplicitWithFlags_Values[q23])
                  | q26 : int(1..12)])
             | q23 : int(1..6)]),
    and([q27 <= s_ExplicitWithRepetition_Flag ->
         sum([toInt(s_ExplicitWithFlags_Values[q28] = s_ExplicitWithRepetition_Values[q27]) *
