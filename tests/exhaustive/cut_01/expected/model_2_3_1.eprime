    and([x_ExplicitVarSizeWithFlags_Flags[q6 + 1] ->
         x_ExplicitVarSizeWithFlags_Flags[q6]
             | q6 : int(1..2)]),
    and([x_ExplicitVarSizeWithFlags_Flags[q19] ->
         or([q21 <= x_ExplicitVarSizeWithMarker_Marker /\
             x_ExplicitVarSizeWithMarker_Values[q21] = x_ExplicitVarSizeWithFlags_Values[q19]
                 | q21 : int(1..3)])
             | q19 : int(1..3)]),
    and([q23 <= x_ExplicitVarSizeWithMarker_Marker ->
         or([x_ExplicitVarSizeWithFlags_Flags[q25] /\
             x_ExplicitVarSizeWithFlags_Values[q25] = x_ExplicitVarSizeWithMarker_Values[q23]
                 | q25 : int(1..3)])
             | q23 : int(1..3)]),
    and([x_Occurrence[q26] ->
         or([q28 <= x_ExplicitVarSizeWithMarker_Marker /\
             x_ExplicitVarSizeWithMarker_Values[q28] = q26
