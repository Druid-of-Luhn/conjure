             | q26 : int(1..3)]),
    and([q30 <= x_ExplicitVarSizeWithMarker_Marker ->
         x_Occurrence[x_ExplicitVarSizeWithMarker_Values[q30]]
             | q30 : int(1..3)]),
    and([x_Occurrence[q31] ->
         or([x_ExplicitVarSizeWithFlags_Flags[q33] /\
             x_ExplicitVarSizeWithFlags_Values[q33] = q31
                 | q33 : int(1..3)])
             | q31 : int(1..3)]),
    and([x_ExplicitVarSizeWithFlags_Flags[q35] ->
         x_Occurrence[x_ExplicitVarSizeWithFlags_Values[q35]]
             | q35 : int(1..3)])

