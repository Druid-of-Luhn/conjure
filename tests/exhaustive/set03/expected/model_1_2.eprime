language ESSENCE' 1.0

find x_Occurrence: matrix indexed by [int(1..2)] of bool
find x_ExplicitVarSizeWithMarker_Marker: int(0..2)
find x_ExplicitVarSizeWithMarker_Values: matrix indexed by [int(1..2)] of int(1..2)
such that
    x_Occurrence[1],
    sum([x_Occurrence[q1] | q1 : int(1..2)]) <= 2,
    2 <= x_ExplicitVarSizeWithMarker_Marker -> x_ExplicitVarSizeWithMarker_Values[1] < x_ExplicitVarSizeWithMarker_Values[2],
    and([q2 > x_ExplicitVarSizeWithMarker_Marker -> x_ExplicitVarSizeWithMarker_Values[q2] = 1 | q2 : int(1..2)]),
    x_ExplicitVarSizeWithMarker_Marker <= 2,
    and([q4 <= x_ExplicitVarSizeWithMarker_Marker -> x_Occurrence[x_ExplicitVarSizeWithMarker_Values[q4]] | q4 : int(1..2)]),
    and([x_Occurrence[q3] -> or([q6 <= x_ExplicitVarSizeWithMarker_Marker /\ x_ExplicitVarSizeWithMarker_Values[q6] = q3 | q6 : int(1..2)]) | q3 : int(1..2)])

