    and([or([aux1_ExplicitVarSizeWithFlags_Flags[q18] /\
             aux1_ExplicitVarSizeWithFlags_Values[q18] = [false, true; int(1..2)][q16]
                 | q18 : int(1..2)])
             | q16 : int(1..2)]),
    and([aux1_ExplicitVarSizeWithFlags_Flags[q14] ->
         or([[false, true; int(1..2)][q20] = aux1_ExplicitVarSizeWithFlags_Values[q14]
                 | q20 : int(1..2)])
             | q14 : int(1..2)]),
    sum([([false, true; int(1..2)][q3] = b) * [5, x; int(1..2)][q3]
             | q3 : int(1..2)])
    = i,
    or([aux1_ExplicitVarSizeWithFlags_Flags[q13] /\
        aux1_ExplicitVarSizeWithFlags_Values[q13] = b
            | q13 : int(1..2)])

