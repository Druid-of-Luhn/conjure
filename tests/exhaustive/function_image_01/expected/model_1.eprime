language ESSENCE' 1.0

find b: bool
find i: int(0..5)
find x: int(1..2)
find aux1_ExplicitVarSizeWithMarker_Marker: int(0..2)
find aux1_ExplicitVarSizeWithMarker_Values:
        matrix indexed by [int(1..2)] of bool
such that
    2 <= aux1_ExplicitVarSizeWithMarker_Marker ->
    aux1_ExplicitVarSizeWithMarker_Values[1] <
    aux1_ExplicitVarSizeWithMarker_Values[2],
    and([q8 > aux1_ExplicitVarSizeWithMarker_Marker ->
         aux1_ExplicitVarSizeWithMarker_Values[q8] = false
             | q8 : int(1..2)]),
    or([q15 <= aux1_ExplicitVarSizeWithMarker_Marker /\
        aux1_ExplicitVarSizeWithMarker_Values[q15] = false
            | q15 : int(1..2)]),
    or([q17 <= aux1_ExplicitVarSizeWithMarker_Marker /\
        aux1_ExplicitVarSizeWithMarker_Values[q17]
            | q17 : int(1..2)]),
    and([q10 <= aux1_ExplicitVarSizeWithMarker_Marker ->
         false = aux1_ExplicitVarSizeWithMarker_Values[q10] \/
         aux1_ExplicitVarSizeWithMarker_Values[q10]
             | q10 : int(1..2)]),
    toInt(false = b) * 5 + toInt(b) * x = i,
    or([q12 <= aux1_ExplicitVarSizeWithMarker_Marker /\
        aux1_ExplicitVarSizeWithMarker_Values[q12] = b
            | q12 : int(1..2)])

