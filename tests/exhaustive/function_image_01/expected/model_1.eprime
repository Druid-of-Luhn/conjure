language ESSENCE' 1.0

find b: bool
find i: int(0..5)
find x: int(1..2)
find aux1_ExplicitVarSizeWithMarker_Marker: int(0..2)
find aux1_ExplicitVarSizeWithMarker_Values:
        matrix indexed by [int(1..2)] of bool
such that
    2 <= aux1_ExplicitVarSizeWithMarker_Marker ->
    aux1_ExplicitVarSizeWithMarker_Values[1] <
    aux1_ExplicitVarSizeWithMarker_Values[2],
    and([q8 > aux1_ExplicitVarSizeWithMarker_Marker ->
         aux1_ExplicitVarSizeWithMarker_Values[q8] = false
             | q8 : int(1..2)]),
    or([q14 <= aux1_ExplicitVarSizeWithMarker_Marker /\
        aux1_ExplicitVarSizeWithMarker_Values[q14] = false
            | q14 : int(1..2)]),
    or([q16 <= aux1_ExplicitVarSizeWithMarker_Marker /\
        aux1_ExplicitVarSizeWithMarker_Values[q16]
            | q16 : int(1..2)]),
    and([q11 <= aux1_ExplicitVarSizeWithMarker_Marker ->
         false = aux1_ExplicitVarSizeWithMarker_Values[q11] \/
         aux1_ExplicitVarSizeWithMarker_Values[q11]
             | q11 : int(1..2)]),
    (false = b) * 5 + b * x = i,
    or([q10 <= aux1_ExplicitVarSizeWithMarker_Marker /\
        aux1_ExplicitVarSizeWithMarker_Values[q10] = b
            | q10 : int(1..2)])

