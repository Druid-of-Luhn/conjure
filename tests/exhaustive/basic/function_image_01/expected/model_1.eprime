language ESSENCE' 1.0

find b: bool
find i: int(0..5)
find x: int(1..2)
find aux1_ExplicitVarSizeWithMarker_Marker: int(0..2)
find aux1_ExplicitVarSizeWithMarker_Values:
        matrix indexed by [int(1..2)] of bool
such that
    2 <= aux1_ExplicitVarSizeWithMarker_Marker ->
    aux1_ExplicitVarSizeWithMarker_Values[1] <
    aux1_ExplicitVarSizeWithMarker_Values[2],
    and([q7 > aux1_ExplicitVarSizeWithMarker_Marker ->
         aux1_ExplicitVarSizeWithMarker_Values[q7] = false
             | q7 : int(1..2)]),
    or([q13 <= aux1_ExplicitVarSizeWithMarker_Marker /\
        aux1_ExplicitVarSizeWithMarker_Values[q13] = false
            | q13 : int(1..2)]),
    or([q13 <= aux1_ExplicitVarSizeWithMarker_Marker /\
        aux1_ExplicitVarSizeWithMarker_Values[q13]
            | q13 : int(1..2)]),
    and([q9 <= aux1_ExplicitVarSizeWithMarker_Marker ->
         false = aux1_ExplicitVarSizeWithMarker_Values[q9] \/
         aux1_ExplicitVarSizeWithMarker_Values[q9]
             | q9 : int(1..2)]),
    toInt(false = b) * 5 + toInt(b) * x = i,
    or([q11 <= aux1_ExplicitVarSizeWithMarker_Marker /\
        aux1_ExplicitVarSizeWithMarker_Values[q11] = b
            | q11 : int(1..2)])

