    and([q24 <= x_ExplicitVarSizeWithMarker_Marker ->
         x_Occurrence[x_ExplicitVarSizeWithMarker_Values[q24]]
             | q24 : int(1..let1)]),
    and([x_Occurrence[q25] ->
         or([q27 <= x_ExplicitVarSizeWithMarker_Marker /\
             x_ExplicitVarSizeWithMarker_Values[q27] = q25
                 | q27 : int(1..let1)])
             | q25 : int(let2..let3)]),
    and([x_ExplicitVarSizeWithDummy[q5] < x_ExplicitVarSizeWithDummy[q5 + 1] \/
         x_ExplicitVarSizeWithDummy[q5] = let3 + 1
             | q5 : int(1..let1 - 1)]),
    and([x_ExplicitVarSizeWithDummy[q6] = let3 + 1 ->
