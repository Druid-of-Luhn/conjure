language ESSENCE' 1.0

find x_ExplicitWithRepetition_Flag: int(0..8)
find x_ExplicitWithRepetition_Values: matrix indexed by [int(1..8)] of int(1..2)
such that
    and([q1 + 1 <= x_ExplicitWithRepetition_Flag ->
         x_ExplicitWithRepetition_Values[q1] <= x_ExplicitWithRepetition_Values[q1 + 1]
             | q1 : int(1..7)]),
    and([q2 > x_ExplicitWithRepetition_Flag ->
         x_ExplicitWithRepetition_Values[q2] = 1
             | q2 : int(1..8)]),
    and([q3 <= x_ExplicitWithRepetition_Flag ->
         sum([toInt(q7 <= x_ExplicitWithRepetition_Flag) *
              toInt(x_ExplicitWithRepetition_Values[q7] = x_ExplicitWithRepetition_Values[q3])
                  | q7 : int(1..8)])
         = 0
         \/
         sum([toInt(q9 <= x_ExplicitWithRepetition_Flag) *
              toInt(x_ExplicitWithRepetition_Values[q9] = x_ExplicitWithRepetition_Values[q3])
                  | q9 : int(1..8)])
         >= 1
             | q3 : int(1..8)]),
    and([q4 <= x_ExplicitWithRepetition_Flag ->
         sum([toInt(q11 <= x_ExplicitWithRepetition_Flag) *
              toInt(x_ExplicitWithRepetition_Values[q11] =
                    x_ExplicitWithRepetition_Values[q4])
                  | q11 : int(1..8)])
         <= 2
             | q4 : int(1..8)])

