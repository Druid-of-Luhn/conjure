language ESSENCE' 1.0

find x_Explicit_ExplicitVarSizeWithDummy:
        matrix indexed by [int(1..2), int(1..3)] of int(1..3)
such that
    x_Explicit_ExplicitVarSizeWithDummy[1, ..] <lex
    x_Explicit_ExplicitVarSizeWithDummy[2, ..],
    and([and([x_Explicit_ExplicitVarSizeWithDummy[q2, q3] <
              x_Explicit_ExplicitVarSizeWithDummy[q2, q3 + 1]
              \/ x_Explicit_ExplicitVarSizeWithDummy[q2, q3] = 3
                  | q3 : int(1..2)])
             | q2 : int(1..2)]),
    and([and([x_Explicit_ExplicitVarSizeWithDummy[q2, q4] = 3 ->
              x_Explicit_ExplicitVarSizeWithDummy[q2, q4 + 1] = 3
                  | q4 : int(1..2)])
             | q2 : int(1..2)]),
    and([sum([toInt(x_Explicit_ExplicitVarSizeWithDummy[q2, q5] != 3)
                  | q5 : int(1..3)])
         <= 3
             | q2 : int(1..2)])

