    and([b_ExplicitWithFlags_Flags[q8 + 1] > 0 -> b_ExplicitWithFlags_Flags[q8] > 0 | q8 : int(1..2)]),
    3 = sum([b_ExplicitWithFlags_Flags[q10] | q10 : int(1..3)])

