language ESSENCE' 1.0

find x_ExplicitVarSizeWithMarker_Marker: int(0..3)
find x_ExplicitVarSizeWithMarker_Values:
        matrix indexed by [int(1..3)] of int(1..4)
find x_Occurrence: matrix indexed by [int(1..4)] of bool
such that
    or([q6 <= x_ExplicitVarSizeWithMarker_Marker /\
        x_ExplicitVarSizeWithMarker_Values[q6] = 1
            | q6 : int(1..3)]),
    or([q8 <= x_ExplicitVarSizeWithMarker_Marker /\
        x_ExplicitVarSizeWithMarker_Values[q8] = 2
            | q8 : int(1..3)]),
    and([q1 + 1 <= x_ExplicitVarSizeWithMarker_Marker ->
         x_ExplicitVarSizeWithMarker_Values[q1] <
         x_ExplicitVarSizeWithMarker_Values[q1 + 1]
             | q1 : int(1..2)]),
    and([q2 > x_ExplicitVarSizeWithMarker_Marker ->
         x_ExplicitVarSizeWithMarker_Values[q2] = 1
             | q2 : int(1..3)]),
    x_ExplicitVarSizeWithMarker_Marker <= 3,
    sum([x_Occurrence[q4] | q4 : int(1..4)]) <= 3,
    and([x_Occurrence[q9] ->
         or([q11 <= x_ExplicitVarSizeWithMarker_Marker /\
             x_ExplicitVarSizeWithMarker_Values[q11] = q9
                 | q11 : int(1..3)])
             | q9 : int(1..4)]),
    and([q13 <= x_ExplicitVarSizeWithMarker_Marker ->
         x_Occurrence[x_ExplicitVarSizeWithMarker_Values[q13]]
             | q13 : int(1..3)])

