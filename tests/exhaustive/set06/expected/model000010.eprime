    and([q4 <= x_ExplicitVarSizeWithMarker_Marker -> x_Occurrence[x_ExplicitVarSizeWithMarker_Values[q4]] | q4 : int(1..4)]),
    and([x_Occurrence[q5] -> or([q6 <= x_ExplicitVarSizeWithMarker_Marker /\ x_ExplicitVarSizeWithMarker_Values[q6] = q5 | q6 : int(1..4)]) | q5 : int(1..4)])

