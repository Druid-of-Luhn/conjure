language ESSENCE' 1.0

find x_RelationAsSet_Explicit_1: matrix indexed by [int(1..4)] of int(1..2)
find x_RelationAsSet_Explicit_2_Explicit:
        matrix indexed by [int(1..4), int(1..2)] of int(1..3)
such that
    and([x_RelationAsSet_Explicit_1[q1] < x_RelationAsSet_Explicit_1[q1 + 1] \/
         x_RelationAsSet_Explicit_1[q1] = x_RelationAsSet_Explicit_1[q1 + 1] /\
         x_RelationAsSet_Explicit_2_Explicit[q1, ..] <lex
         x_RelationAsSet_Explicit_2_Explicit[q1 + 1, ..]
             | q1 : int(1..3)]),
    and([x_RelationAsSet_Explicit_2_Explicit[q2, 1] <
         x_RelationAsSet_Explicit_2_Explicit[q2, 2]
             | q2 : int(1..4)])

