language ESSENCE' 1.0

letting let1 be 5
find var3_ExplicitVarSizeWithFlags_Flags:
        matrix indexed by [int(1..let1)] of bool
find var3_ExplicitVarSizeWithFlags_Values:
        matrix indexed by [int(1..let1)] of bool
such that
    and([var3_ExplicitVarSizeWithFlags_Flags[q1 + 1] ->
         var3_ExplicitVarSizeWithFlags_Values[q1] <
         var3_ExplicitVarSizeWithFlags_Values[q1 + 1]
             | q1 : int(1..let1 - 1)]),
    and([var3_ExplicitVarSizeWithFlags_Flags[q2] = false ->
         var3_ExplicitVarSizeWithFlags_Values[q2] = false
             | q2 : int(1..let1)]),
    and([var3_ExplicitVarSizeWithFlags_Flags[q3 + 1] ->
         var3_ExplicitVarSizeWithFlags_Flags[q3]
             | q3 : int(1..let1 - 1)]),
    sum([toInt(var3_ExplicitVarSizeWithFlags_Flags[q4]) | q4 : int(1..let1)]) <=
    let1

