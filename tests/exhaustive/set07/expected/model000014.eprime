language ESSENCE' 1.0

find x_ExplicitVarSizeWithMarker_Marker: int(0..1 + 4 - 1)
find x_ExplicitVarSizeWithMarker_Values: matrix indexed by [int(1..1 + 4 - 1)] of int(1..4)
such that
    and([or([q4 <= x_ExplicitVarSizeWithMarker_Marker /\ x_ExplicitVarSizeWithMarker_Values[q4] = q2 | q4 : int(1..1 + 4 - 1)]) /\
         !or([q5 <= x_ExplicitVarSizeWithMarker_Marker /\ x_ExplicitVarSizeWithMarker_Values[q5] = q3 | q5 : int(1..1 + 4 - 1)])
             | q2 : int(1..3), q3 : int(4..9)]),
    and([q1 + 1 <= x_ExplicitVarSizeWithMarker_Marker -> x_ExplicitVarSizeWithMarker_Values[q1] < x_ExplicitVarSizeWithMarker_Values[q1 + 1]
             | q1 : int(1..1 + 4 - 1 - 1)]),
    and([q1 > x_ExplicitVarSizeWithMarker_Marker -> x_ExplicitVarSizeWithMarker_Values[q1] = 1 | q1 : int(1..1 + 4 - 1)])

