    and([q3 <= x_ExplicitVarSizeWithMarker_Marker -> x_Occurrence[x_ExplicitVarSizeWithMarker_Values[q3]] | q3 : int(1..1 + (4 - 1))]),
    and([x_Occurrence[q2] -> or([q5 <= x_ExplicitVarSizeWithMarker_Marker /\ x_ExplicitVarSizeWithMarker_Values[q5] = q2 | q5 : int(1..1 + (4 - 1))])
             | q2 : int(1..4)])

