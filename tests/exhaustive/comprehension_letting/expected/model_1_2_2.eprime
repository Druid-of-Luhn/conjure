language ESSENCE' 1.0

find s_Occurrence: matrix indexed by [int(1..4)] of bool
find s_ExplicitVarSizeWithMarker_Marker: int(0..4)
find s_ExplicitVarSizeWithMarker_Values:
        matrix indexed by [int(1..4)] of int(1..4)
find x: int(-100..100)
find aux1: int(-16..3)
such that
    and([and([s_Occurrence[i], q5 <= s_ExplicitVarSizeWithMarker_Marker,
              i != s_ExplicitVarSizeWithMarker_Values[q5],
              allDiff([i + s_ExplicitVarSizeWithMarker_Values[q5],
                       i * s_ExplicitVarSizeWithMarker_Values[q5],
                       i / s_ExplicitVarSizeWithMarker_Values[q5]; int(1..3)]),
              (i - s_ExplicitVarSizeWithMarker_Values[q5]) % 2 = 0; int(1..5)])
         ->
         min([i + s_ExplicitVarSizeWithMarker_Values[q5],
              i - s_ExplicitVarSizeWithMarker_Values[q5],
              i * s_ExplicitVarSizeWithMarker_Values[q5],
              i / s_ExplicitVarSizeWithMarker_Values[q5]; int(1..4)])
         <= aux1
             | i : int(1..4), q5 : int(1..4)]),
    or([and([s_Occurrence[i], q5 <= s_ExplicitVarSizeWithMarker_Marker,
             i != s_ExplicitVarSizeWithMarker_Values[q5],
             allDiff([i + s_ExplicitVarSizeWithMarker_Values[q5],
                      i * s_ExplicitVarSizeWithMarker_Values[q5],
                      i / s_ExplicitVarSizeWithMarker_Values[q5]; int(1..3)]),
             (i - s_ExplicitVarSizeWithMarker_Values[q5]) % 2 = 0; int(1..5)])
        /\
        min([i + s_ExplicitVarSizeWithMarker_Values[q5],
             i - s_ExplicitVarSizeWithMarker_Values[q5],
             i * s_ExplicitVarSizeWithMarker_Values[q5],
             i / s_ExplicitVarSizeWithMarker_Values[q5]; int(1..4)])
        = aux1
            | i : int(1..4), q5 : int(1..4)]),
    x = aux1,
    and([q2 + 1 <= s_ExplicitVarSizeWithMarker_Marker ->
         s_ExplicitVarSizeWithMarker_Values[q2] <
         s_ExplicitVarSizeWithMarker_Values[q2 + 1]
             | q2 : int(1..3)]),
    and([q3 > s_ExplicitVarSizeWithMarker_Marker ->
         s_ExplicitVarSizeWithMarker_Values[q3] = 1
             | q3 : int(1..4)]),
    and([q7 <= s_ExplicitVarSizeWithMarker_Marker ->
         s_Occurrence[s_ExplicitVarSizeWithMarker_Values[q7]]
             | q7 : int(1..4)]),
    and([s_Occurrence[q8] ->
         or([q10 <= s_ExplicitVarSizeWithMarker_Marker /\
             s_ExplicitVarSizeWithMarker_Values[q10] = q8
                 | q10 : int(1..4)])
             | q8 : int(1..4)])

