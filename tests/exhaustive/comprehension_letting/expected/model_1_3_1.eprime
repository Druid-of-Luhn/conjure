language ESSENCE' 1.0

find s_Occurrence: matrix indexed by [int(1..4)] of bool
find s_ExplicitVarSizeWithFlags_Flags: matrix indexed by [int(1..4)] of bool
find s_ExplicitVarSizeWithFlags_Values:
        matrix indexed by [int(1..4)] of int(1..4)
find x: int(-100..100)
find aux1: int(-16..3)
such that
    and([and([s_Occurrence[i], s_ExplicitVarSizeWithFlags_Flags[q7],
              i != s_ExplicitVarSizeWithFlags_Values[q7],
              allDiff([i + s_ExplicitVarSizeWithFlags_Values[q7],
                       i * s_ExplicitVarSizeWithFlags_Values[q7],
                       i / s_ExplicitVarSizeWithFlags_Values[q7]; int(1..3)]),
              (i - s_ExplicitVarSizeWithFlags_Values[q7]) % 2 = 0; int(1..5)])
         ->
         min([i + s_ExplicitVarSizeWithFlags_Values[q7],
              i - s_ExplicitVarSizeWithFlags_Values[q7],
              i * s_ExplicitVarSizeWithFlags_Values[q7],
              i / s_ExplicitVarSizeWithFlags_Values[q7]; int(1..4)])
         <= aux1
             | i : int(1..4), q7 : int(1..4)]),
    or([and([s_Occurrence[i], s_ExplicitVarSizeWithFlags_Flags[q7],
             i != s_ExplicitVarSizeWithFlags_Values[q7],
             allDiff([i + s_ExplicitVarSizeWithFlags_Values[q7],
                      i * s_ExplicitVarSizeWithFlags_Values[q7],
                      i / s_ExplicitVarSizeWithFlags_Values[q7]; int(1..3)]),
             (i - s_ExplicitVarSizeWithFlags_Values[q7]) % 2 = 0; int(1..5)])
        /\
        min([i + s_ExplicitVarSizeWithFlags_Values[q7],
             i - s_ExplicitVarSizeWithFlags_Values[q7],
             i * s_ExplicitVarSizeWithFlags_Values[q7],
             i / s_ExplicitVarSizeWithFlags_Values[q7]; int(1..4)])
        = aux1
            | i : int(1..4), q7 : int(1..4)]),
    x = aux1,
    and([s_ExplicitVarSizeWithFlags_Flags[q2 + 1] ->
         s_ExplicitVarSizeWithFlags_Values[q2] <
         s_ExplicitVarSizeWithFlags_Values[q2 + 1]
             | q2 : int(1..3)]),
    and([s_ExplicitVarSizeWithFlags_Flags[q3] = false ->
         s_ExplicitVarSizeWithFlags_Values[q3] = 1
             | q3 : int(1..4)]),
    and([s_ExplicitVarSizeWithFlags_Flags[q4 + 1] ->
         s_ExplicitVarSizeWithFlags_Flags[q4]
             | q4 : int(1..3)]),
    and([s_ExplicitVarSizeWithFlags_Flags[q9] ->
         s_Occurrence[s_ExplicitVarSizeWithFlags_Values[q9]]
             | q9 : int(1..4)]),
    and([s_Occurrence[q10] ->
         or([s_ExplicitVarSizeWithFlags_Flags[q12] /\
             s_ExplicitVarSizeWithFlags_Values[q12] = q10
                 | q12 : int(1..4)])
             | q10 : int(1..4)])

