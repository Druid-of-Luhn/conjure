    or([q5 <= var2_ExplicitVarSizeWithMarker_Marker /\
        !or([q8 <= var2_ExplicitVarSizeWithMarker_Marker /\
             var2_ExplicitVarSizeWithMarker_Values[q8] =
             var2_ExplicitVarSizeWithMarker_Values[q5]
