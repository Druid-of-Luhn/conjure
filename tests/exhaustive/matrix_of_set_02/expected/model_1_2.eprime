language ESSENCE' 1.0

find x_Occurrence: matrix indexed by [int(1..2), int(1..3)] of bool
find x_ExplicitVarSizeWithMarker_Marker:
        matrix indexed by [int(1..2)] of int(0..3)
find x_ExplicitVarSizeWithMarker_Values:
        matrix indexed by [int(1..2), int(1..3)] of int(1..3)
such that
    and([x_Occurrence[j, i] -> i % 2 = 0 | j : int(1..2), i : int(1..3)]),
    and([and([q2 + 1 <= x_ExplicitVarSizeWithMarker_Marker[q1] ->
              x_ExplicitVarSizeWithMarker_Values[q1, q2] <
              x_ExplicitVarSizeWithMarker_Values[q1, q2 + 1]
                  | q2 : int(1..2)])
             | q1 : int(1..2)]),
    and([and([q2 > x_ExplicitVarSizeWithMarker_Marker[q1] ->
              x_ExplicitVarSizeWithMarker_Values[q1, q2] = 1
                  | q2 : int(1..3)])
             | q1 : int(1..2)]),
    and([and([q5 <= x_ExplicitVarSizeWithMarker_Marker[q3] ->
              x_Occurrence[q3, x_ExplicitVarSizeWithMarker_Values[q3, q5]]
                  | q5 : int(1..3)])
         /\
         and([x_Occurrence[q3, q4] ->
              or([q7 <= x_ExplicitVarSizeWithMarker_Marker[q3] /\
                  x_ExplicitVarSizeWithMarker_Values[q3, q7] = q4
                      | q7 : int(1..3)])
                  | q4 : int(1..3)])
             | q3 : int(1..2)])

