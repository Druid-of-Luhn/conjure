language ESSENCE' 1.0

find s_Occurrence: matrix indexed by [int(1..2)] of bool
find s_ExplicitVarSizeWithMarker_Marker: int(0..1 + (2 - 1))
find s_ExplicitVarSizeWithMarker_Values: matrix indexed by [int(1..1 + (2 - 1))] of int(1..2)
such that
    or([q2 <= s_ExplicitVarSizeWithMarker_Marker /\ s_ExplicitVarSizeWithMarker_Values[q2] = sum([s_Occurrence[q3] * 1 | q3 : int(1..2)])
            | q2 : int(1..1 + (2 - 1))]),
    and([q1 + 1 <= s_ExplicitVarSizeWithMarker_Marker -> s_ExplicitVarSizeWithMarker_Values[q1] < s_ExplicitVarSizeWithMarker_Values[q1 + 1]
             | q1 : int(1..1 + (2 - 1) - 1)]),
    and([q1 > s_ExplicitVarSizeWithMarker_Marker -> s_ExplicitVarSizeWithMarker_Values[q1] = 1 | q1 : int(1..1 + (2 - 1))]),
    and([q4 <= s_ExplicitVarSizeWithMarker_Marker -> s_Occurrence[s_ExplicitVarSizeWithMarker_Values[q4]] | q4 : int(1..1 + (2 - 1))]),
    and([s_Occurrence[q5] -> or([q6 <= s_ExplicitVarSizeWithMarker_Marker /\ s_ExplicitVarSizeWithMarker_Values[q6] = q5 | q6 : int(1..1 + (2 - 1))])
             | q5 : int(1..2)])

