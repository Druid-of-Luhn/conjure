    and([s_ExplicitVarSizeWithFlags_Flags[q3] -> s_Occurrence[s_ExplicitVarSizeWithFlags_Values[q3]] | q3 : int(1..1 + (2 - 1))]),
    and([s_Occurrence[q4] -> or([s_ExplicitVarSizeWithFlags_Flags[q5] /\ s_ExplicitVarSizeWithFlags_Values[q5] = q4 | q5 : int(1..1 + (2 - 1))]) | q4 : int(1..2)])

