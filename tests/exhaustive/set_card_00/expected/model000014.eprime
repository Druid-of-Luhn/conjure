language ESSENCE' 1.0

find s_ExplicitVarSizeWithMarker_Marker: int(0..1 + (2 - 1))
find s_ExplicitVarSizeWithMarker_Values: matrix indexed by [int(1..1 + (2 - 1))] of int(1..2)
such that
    or([q2 <= s_ExplicitVarSizeWithMarker_Marker /\ s_ExplicitVarSizeWithMarker_Values[q2] = s_ExplicitVarSizeWithMarker_Marker | q2 : int(1..1 + (2 - 1))]),
    and([q1 + 1 <= s_ExplicitVarSizeWithMarker_Marker -> s_ExplicitVarSizeWithMarker_Values[q1] < s_ExplicitVarSizeWithMarker_Values[q1 + 1]
             | q1 : int(1..1 + (2 - 1) - 1)]),
    and([q1 > s_ExplicitVarSizeWithMarker_Marker -> s_ExplicitVarSizeWithMarker_Values[q1] = 1 | q1 : int(1..1 + (2 - 1))])

