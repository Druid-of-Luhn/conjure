language ESSENCE' 1.0

find var1_Function1DPartial_Flags: matrix indexed by [bool] of bool
find var1_Function1DPartial_Values: matrix indexed by [bool] of bool
such that
    or([var1_Function1DPartial_Flags[q5] /\
        !(q5 /\ false = var1_Function1DPartial_Values[q5])
            | q5 : bool])
    \/
    !or([var1_Function1DPartial_Flags[q12] /\
         (q12 /\ var1_Function1DPartial_Values[q12] = false)
             | q12 : bool]),
    and([var1_Function1DPartial_Flags[q1] = false ->
         var1_Function1DPartial_Values[q1] = false
             | q1 : bool])

