language Essence 2.0

letting digit be domain int(0..9)

find x : digit
find y : digit

letting xy be x * y

letting myForall be quantifier {
    append  { x:bool, y:bool -> x /\ y }
    guard   { x:bool, y:bool -> x => y }
    identity true }

letting mySum be quantifier {
    append   { x:int , y:int -> x + y }
    guard    { x:bool, y:int -> x * y }
    identity 0 }

such that
    myForall i : int(1..9) . xy > i,
    (mySum i : int(1..9) . i) > xy
