language Essence 2.0

letting myForall be quantifier { lambda { x : bool, y : bool -> x /\ y }
                                 lambda { x : bool, y : bool -> x => y }
                                 true
                               }
find s : set of int(1..90)

such that
    myForall i : int(1..9) . myForall j : int(1..9) , i = 2 * j . i + j elem s
