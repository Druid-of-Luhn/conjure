language Essence 2.0

letting digit be domain int(0..9)
find x : digit
find y : digit
letting xy be x * y
letting forall_glueOp be lambda { x : bool, y : bool -> x /\ y }
letting forall_skipOp be lambda { x : bool, y : bool -> x => y }
letting forall be quantifier { forall_glueOp
                               forall_glueOp
                               true
                             }

such that
    x = 3 * y,
    xy > 0
